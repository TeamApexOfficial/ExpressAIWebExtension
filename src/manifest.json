{
	"name": "Express AI",
	"version": "1.0.0",
	"description": "Text-to-speech with sentence highlighting",
	"manifest_version": 3,
	"permissions": ["scripting", "activeTab"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": "icon.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"host_permissions": ["<all_urls>"]
}
